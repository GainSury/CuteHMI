#include(compat.qdocconf)

project = CuteHMI
description = CuteHMI QML module

sourcedirs = .

sources.fileextensions = "*.cpp *.qdoc *.mm *.qml"
headers.fileextensions = "*.h *.ch *.h++ *.hh *.hpp *.hxx"
examples.fileextensions = "*.cpp *.h *.js *.xq *.svg *.xml *.ui *.qhp *.qhcp *.qml"
examples.imageextensions = "*.png *.jpeg *.jpg *.gif *.mng"

outputdir  =    ../../../doc/CuteHMI/QML/CuteHMI/html
outputformats = HTML

HTML.stylesheets = ../../doc/template/style/qt.css
HTML.headerstyles = \
    "  <link rel=\"stylesheet\" type=\"text/css\" href=\"style/qt.css\" />\n"

