deploy: $(PRODUCT_INCLUDE_FILES_DEPLOY) $(PRODUCT_LIB_FILES_DEPLOY)

$(PRODUCT_INCLUDE_FILES_DEPLOY): $(PRODUCT_INCLUDE_FILES_INSTALL) | $(MKDIR) $(CP)
	$(MKDIR) -p $(PRODUCT_DEPLOY_INCLUDE_DIR)
	$(CP) -Rf $? $(PRODUCT_DEPLOY_INCLUDE_DIR)

$(PRODUCT_LIB_FILES_DEPLOY): $(PRODUCT_LIB_FILES_INSTALL) | $(MKDIR) $(CP)
	$(MKDIR) -p $(PRODUCT_DEPLOY_LIB_DIR)
	$(CP) -Rf $? $(PRODUCT_DEPLOY_LIB_DIR)

